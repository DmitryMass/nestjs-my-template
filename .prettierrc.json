{
  "plugins": [
      "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
      "react",
      "<THIRD_PARTY_MODULES>",
      "^./(.*)$",
      "^../(.*)$",
      "@components/",
      "@utils/",
      "@types/"
  ],
  "printWidth": 80,
  "trailingComma": "all",
  "importOrderSeparation":true,
  "importOrderSortSpecifiers":true,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true
}